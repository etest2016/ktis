				<HyperLink show="true"/>
				<Image show="true"/>
				<Images show="true"/>
				<HorizontalLine show="true"/>
				<Layer show="true"/>
				<Symbol show="true"/>
				<Attachment show="false"/>
				<Separator show="true"/>
				<Object show="true">
					<Flash show="true"/>
					<WindowsMedia show="true"/>
					<ActiveX show="true"/>
					<Separator show="true"/>
					<Plugin show="true"/>
				</Object>
			<PageBreak show="false"/>
			</Insert>
			<View show="true">
				<AbsolutePosition show="true"/>
				<ShowGrid show="true" state="off"/>
				<ShowTag show="true" state="off"/>
				<SnapToGrid show="true" state="off"/>
				<ShowZeroBorder show="true" state="on"/>
				<Separator show="true"/>
				<Encoding show="true"/>
			</View>
			<Table show="true">    
				<InsertTable show="true"/>
				<InsertRowColumn show="true"/>
				<InsertCell show="true"/>
				<Separator show="true"/>
				<DeleteRow show="true"/>
				<DeleteColumn show="true"/>
				<DeleteCell show="true"/>
				<Separator show="true"/>
				<MergeCell show="true"/>
				<SplitCell show="true"/>
				<Separator show="true"/>
				<TableCellBucket show="true"/>
				<TableRowsEvenly show="true"/>
				<TableColumnsEvenly show="true"/>			
				<Separator show="true"/>
				<AlignUpper show="true"/>
				<AlignVCenter show="true"/>
				<AlignLower show="true"/>
				<Separator show="true"/>
				<ClearTableClipboard show="true"/>
			</Table>
			<Style show="true">       
				<Bold show="true"/>
				<Italic show="true"/>
				<UnderLine show="true"/>
				<Separator show="true"/>
				<BulletedList show="true"/>
				<NumberedList show="true"/>
				<Separator show="true"/>
				<IncreaseIndent show="true"/>
				<DecreaseIndent show="true"/>
				<Separator show="true"/>
				<Align show="true">
					<AlignLeft show="true"/>
					<AlignCenter show="true"/>
					<AlignRight show="true"/>
				</Align>
			</Style>
			<Tool show="true">
				<PageProperty show="true"/>
				<SourceTabProperty show="true"/>
				<Stationery show="false" state="off"/>
			</Tool>
			<Help show="true">
				<About show="true"/>
			</Help>
		<CustomMenu name="사용자정의" show="false">
			<CustomMenuItem name="도움말" id="1" show="true"/>
			<CustomMenuItem name="메뉴1" id="2" show="true"/>
			<CustomMenuItem name="메뉴2" id="3" show="true"/>
		</CustomMenu>
		</ShowMenu>
		
		<!-- 보여줄 툴바 지정 < 1 : 첫 번째 툴바, 2 : 두 번째 툴바 > -->
		<ShowToolBar index="1" show="true" state="on">
			<New show="false"/>
			<Open show="false"/>
			<Save show="false"/>
			<SaveAs show="false"/>
			<Print show="false"/>
			<Separator show="false"/>
			<Cut show="true"/>
			<Copy show="true"/>
			<Paste show="true"/>
			<Separator show="true"/>
			<Undo show="false"/>
			<Redo show="false"/>
			<Separator show="false"/>
			<DrawTable show="false"/>
			<EraseTable show="false"/>			
			<InsertTableGrid show="true"/>
			<Separator show="true"/>
			<CellMergeSplit show="false"/>
			<CellInsertDelete show="false"/>
			<MergeCell show="false"/>
			<SplitCell show="false"/>
			<Separator show="false"/>			
			<TableRowsEvenly show="false"/>
			<TableColumnsEvenly show="false"/>
			<Separator show="false"/>
			<AlignUpper show="false"/>
			<AlignVCenter show="false"/>
			<AlignLower show="false"/>
			<Separator show="false"/>
			<HyperLink show="false"/>
			<Attachment show="false"/>
			<Image show="true"/>
			<Images show="false"/>
			<Flash show="true"/>
			<WindowsMedia show="true"/>
			<Separator show="true"/>
            <StyleCombo show="false"/>
			<FontEnumCombo show="false"/>
			<FontSizeCombo show="false"/>
			<Separator show="false"/>
			<Bold show="true"/>
			<Italic show="true"/>
			<Underline show="true"/>
			<Separator show="true"/>
			<TextColor show="true"/>
			<TextBackgroundColor show="false"/>
			<Separator show="false"/>
			<AlignLeft show="false"/>
			<AlignCenter show="false"/>
			<AlignRight show="false"/>
			<Separator show="true"/>
			<Symbol show="true"/>	
		</ShowToolBar>
		
		<ShowToolBar index="2" show="false" state="on">			
			<StyleCombo show="true"/>
			<FontEnumCombo show="true"/>
			<FontSizeCombo show="true"/>			
			<LineHeightCombo show="true" default=""/><!-- default : Floating-point number, percentage ex) 1 or 100%-->
			<LetterSpacingCombo show="false"/>
			<Separator show="true"/>
			<Bold show="true"/>
			<Italic show="true"/>
			<Underline show="true"/>
			<Separator show="true"/>
			<TextColor show="true"/>
			<TextBackgroundColor show="true"/>
			<TableCellBucket show="true"/>
			<Separator show="true"/>
			<AlignLeft show="true"/>
			<AlignCenter show="true"/>
			<AlignRight show="true"/>
			<Justify show="false"/>		
			<Distributed show="false"/>
			<Separator show="true"/>
			<DecreaseIndent show="true"/>
			<IncreaseIndent show="true"/>
			<Separator show="false"/>
			<BulletedList show="true"/>
			<NumberedList show="true"/>
			<Separator show="true"/>
			<Symbol show="true"/>			
			<AbsolutePosition show="true"/>
			<ShowGrid show="true"/>
			<ShowZeroBorder show="true"/>
			<PageBreak show="false"/>
			<!--admin버전에만 적용-->
			<ToggleAdminMode show="false"/>
			<ReadOnly show="false"/>
			<Editable show="false"/>			
		</ShowToolBar>
			
		<!-- 보여줄 탭 지정 < source : HTML 소스 편집 탭 표시, preview : Preview 탭 표시, Design 탭은 항상 표시 > -->
		<ShowTab show="true">source</ShowTab>
		<!-- default = system language -->
		<!-- 나올수 있는 값, english, korean, japanese, chinese_simplified, chinese_traditional unknown:default -->
		<Language apply="false">japanese</Language>
		<!-- 컨트롤의 외곽스타일(flat, sunkun..) (notimpl)-->
		<ControlBorder>notimpl</ControlBorder>
		<!-- 뷰어모드에서의 스크롤-->
		<ViewModeHideScroll>false</ViewModeHideScroll>
	</UI>
	<Edit>
		<!-- 페이지 초기 설정 및 편집에 관련된 초기 설정 적용, ApplyInitData 파라미터의 값이 1일 경우에만 적용 -->		
		<!-- HTML charset(설정 가능 항목 : http://www.iana.org/assignments/character-sets)-->
		<Charset apply="true">euc-kr</Charset>
		<!-- 작성하는 HTML 문서의 제목 -->
		<Title apply="false">QmTm 문제</Title>
		<!-- HTML 디자인 및 편집 가능 여부 < on : 편집 가능, off : 편집 불가 > -->
		<EditMode apply="false">on</EditMode>
		<!-- HTML 디자인 탭에서의 편집 시 탭 키에 대한 공백 문자열 개수 지정 -->
		<TabSize apply="false"></TabSize>
		<!-- HTML 디자인 탭에서의 편집 시 문서의 초기 백그라운드 색상 지정 < 16진수로 지정, ex)#ffffff > -->
		<BgColor apply="false"></BgColor>
		<!-- HTML 디자인 탭에서의 편집 시 문서의 초기 백그라운드 이미지 지정 : URL로 지정함. -->
		<BgImage apply="false"></BgImage>
		<!-- HTML 디자인 탭에서의 편집 시 초기 폰트 색상 지정 < 16진수로 지정, ex)#ffffff > -->
		<FontColor apply="false"></FontColor>
		<!-- HTML 디자인 탭에서의 편집 시 초기 폰트 종류 지정 -->
		<FontFamily apply="false"></FontFamily>
		<!-- HTML 디자인 탭에서의 편집 시 초기 폰트 크기 지정, ex)12pt, 12px -->
		<FontSize apply="false"></FontSize>
		<!-- Grid 선택 시 Grid의 색상 < 16 진수로 지정, ex)#ffffff > -->
		<GridColor apply="false">#000000</GridColor>
		<!-- Grid 선택 시 Grid 선의 타입 < line : 선, dot : 점선 > -->
		<GridShape apply="false">dot</GridShape>
		<!-- Grid 선택 시 Grid 가로 격자 간격 : 픽셀로 지정 -->
		<GridSize apply="false">100</GridSize>
		<!-- Grid 선택 시 Grid 세로 격자 간격 : 픽셀로 지정 -->
		<GridHeightSize apply="false">200</GridHeightSize>
		<!-- Grid 선택 시 Grid 가로 여백 : 픽셀로 지정 -->
		<GridMarginSize apply="false">50</GridMarginSize>
		<!-- Grid 선택 시 Grid 세로 여백 : 픽셀로 지정 -->
		<GridHeightMarginSize apply="false">10</GridHeightMarginSize>
		<!-- Snap 옵션 선택 시 Snap 크기 설정 : 픽셀로 지정, Grid 격자 간격의 배수로 설정하는 것이 유용 -->
		<SnapSize apply="false">50</SnapSize>
		<!-- 테이블의 삽입, 드로잉시에 적용될 테이블의 기본적인 속성 지정 -->
		<!-- <Table>태그에 속성을 지정하는 방식과 동일하게 속성과 속성값을 지정(대소문자 구분) -->
		<!-- 지정 가능한 속성 : background, bgColor, border, borderColor, borderColorDark, borderColorLight, cellPadding, cellSpacing -->
		<TableStyle apply="false" background="" bgColor="white" border="1" borderColor="#6E6E6E" borderColorDark="" borderColorLight="" cellPadding="0" cellSpacing="0" align="left" style="BORDER-COLLAPSE: collapse;BORDER-RIGHT: medium none; BORDER-TOP: medium none;BORDER-BOTTOM: medium none; BORDER-LEFT: medium none;"/>
		<!-- <TD>태그에 속성을 지정하는 방식과 동일하게 속성과 속성값을 지정(대소문자 구분) -->
		<TDStyle apply="false" bgColor="" borderColor="" align="" noWrap="" style="BORDER-RIGHT: #000000 1pt solid; BORDER-TOP: #000000 1pt solid; BORDER-LEFT: #000000 1pt solid; BORDER-BOTTOM: #000000 1pt solid"/>		
		<!--외부스타일 시트 링크-->
		<StyleSheetLink apply="false" href="http://www.tagfree.com/tagfree.css"/>
		<!--첨부파일들의 한도 사이즈, 단위 byte , 무제한 : apply=false 또는 값 -1(음수값)-->
		<AttachmentLimit apply="false">1048576</AttachmentLimit>
		<!--Base태그 표현 설정 < true : 안보임, false : 보임 ,defalut:false > -->
		<HideBaseElement apply="false">false</HideBaseElement>		
		<!-- Image Max Size < limit : 한계 기준, fixed : 기준 초과시 지정될 사이즈 > -->		
		<ImageSize apply="true" limit="400" fixed="500"/>		
		<!-- Image Resizing 비율 처리 여부 < true : 비율처리, false : 일반처리 ,defalut:false > -->
		<ImageResizingRate apply="true">true</ImageResizingRate>		
		<!-- Attach 관련 설정 -->		
		<!-- 첨부 기능 , class : 첨부태그 클래스명, style : 첨부태그 스타일, src : 기본 이미지, drop : 드래그앤드랍 첨부(true,false), paste : 붙여넣기 첨부(true,false), position : 첨부 위치(any,top) -->
		<!-- name의 형식 : ".확장자" -->
		<Attachment apply="false" class="twe_attach" style="font-size:9pt;" src="http://tagfree.com/icon/blank.gif" drop="false" paste="false" position="any">
			<Extension name=".xls">http://tagfree.com/icon/xls.gif</Extension>
			<Extension name=".doc">http://tagfree.com/icon/doc.gif</Extension>
			<Extension name=".txt">http://tagfree.com/icon/txt.gif</Extension>
			<Extension name=".pdf">http://tagfree.com/icon/pdf.gif</Extension>
			<Extension name=".hwp">http://tagfree.com/icon/hwp.gif</Extension>
			<Extension name=".ppt">http://tagfree.com/icon/ppt.gif</Extension>
			<Extension name=".avi">http://tagfree.com/icon/media.gif</Extension>
			<Extension name=".mp3">http://tagfree.com/icon/media.gif</Extension>
			<Extension name=".wma">http://tagfree.com/icon/media.gif</Extension>
			<Extension name=".wmv">http://tagfree.com/icon/media.gif</Extension>
			<Extension name=".asf">http://tagfree.com/icon/media.gif</Extension>
			<Extension name=".mpg">http://tagfree.com/icon/media.gif</Extension>
		</Attachment>
		<!-- DefaultStyleCSS 기본 스타일시트를 설정한다. < true : 적용, false : 미적용 > -->
		<DefaultStyleCSS apply="false">			
			P {
				MARGIN-TOP: 0pt; MARGIN-BOTTOM: 0pt
			}
			BLOCKQUOTE {
				MARGIN-TOP: 0pt; MARGIN-BOTTOM: 0pt
			}
			FONT {
				FONT-SIZE: 10pt
			}
			TD {
				WORD-BREAK: break-all
			}
		</DefaultStyleCSS>
		<!-- 이미지 프린트 사용자 미리보기 설정 및 크기 : 가로 격자 크기 설정 : 픽셀로 지정 -->
  	<DraftFormPrintPreview apply="false">500</DraftFormPrintPreview>
	</Edit>
	<Mime>
		<!-- HTML 인코딩 시에 사용할 인코딩 타입 설정 < base64 : BASE64 인코딩, qp : Quoted-Printable 인코딩 > -->
		<HtmlEncodingType>base64</HtmlEncodingType>
		<!-- HTML 인코딩 시에 인코딩 할 리소스 파일의 종류 설정 < img : img 태그, bgsound : body 태그의 bgsound 속성 > -->
		<!-- 추가 : anchor는 a태그의 href를 업로드, include의 link, script 업로드 -->
		<!--<EncodingFileType>image, bgsound, multimedia, anchor, link, script</EncodingFileType>-->
		<EncodingFileType>image, bgsound, multimedia</EncodingFileType>
		<!-- 마임값으로 생성할 파일의 범위 < all : 로컬파일+원격파일, local : 로컬파일, 기본값 : 로컬파일 > -->
		<EncodingFileRange showProgress="false">local</EncodingFileRange>
		<!-- 마임값으로 생성할 파일중 허용될 대상 -->
		<EncodingFileAcception apply="false">
			<Pattern>.jsp</Pattern>
			<!--<Pattern>http://localhost</Pattern>-->
			<!--<Pattern>file://c:/</Pattern>-->
			<!--<Pattern>file:///c:/</Pattern>-->
			<!--<Pattern>file:///C:/</Pattern>-->
		</EncodingFileAcception>
		<!-- 마임값으로 생성할 파일중 제외될 대상 -->
		<EncodingFileException>
			<Pattern>http://tagfree.com/upload</Pattern>
			<Pattern>http://localhost/object.asp</Pattern>
			<Pattern>http://tagfree.com/icon</Pattern>
			<!--<Pattern>.bmp</Pattern>-->
			<!--<Pattern>file://c:/</Pattern>-->
			<!--<Pattern>file:///c:/</Pattern>-->
			<!--<Pattern>file:///C:/</Pattern>-->
		</EncodingFileException>

		<!-- HTML 인코딩 시에 인코딩 범위 < inbody : Body 태그 내부만, outbody : Body 태그 및 그 내부, all : 전체 HTML 소스 -->
		<EncodingRange>inbody</EncodingRange>
	</Mime>
	<System>
		<!-- Output MarkupLanguage 포맷형식 < html, xml >-->
		<MarkupLanguage>html</MarkupLanguage>
	</System>
</TWEditor>