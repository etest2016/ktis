			</File>
			<Edit show="true">
				<Undo show="true"/>
				<Redo show="true"/>
				<Separator show="true"/>
				<Cut show="true"/>
				<Copy show="true"/>
				<Paste show="true"/>
				<Separator show="true"/>
				<Delete show="true"/>
				<SelectAll show="true"/>
				<Separator show="true"/>
				<Find show="true"/>
				<Replace show="true"/>
				<Separator show="true"/>
				<WordWrap show="true"/>
				<Separator show="true"/>
				<TagDiet show="true"/>				
			</Edit>
			<Insert show="true">
				<TextBox show="true"/>
				<TextRange show="true"/>
				<Password show="true"/>
				<Hidden show="true"/>
				<DropDown show="true"/>
				<ListBox show="true"/>
				<CheckBox show="true"/>
